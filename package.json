{
  "name": "penpot-devtools",
  "description": "Penpot Devtools",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/dist",
    "build:dev": "vite build --base=/dist -m development",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "test": "vitest",
    "test:dev": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@codemirror/state": "^6.4.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.25.1",
    "@nextjournal/clojure-mode": "^0.3.1",
    "install": "^0.13.0",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.1",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@pinia/testing": "^0.1.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "sass": "^1.71.1",
    "vite": "link:@intlify/unplugin-vue-i18n/vite",
    "vitest": "^1.3.1"
  }
}
