{
  "name": "penpot-devtools",
  "description": "Penpot Devtools",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/dist",
    "build:dev": "vite build --base=/dist -m development",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "test": "vitest",
    "test:dev": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.11",
    "@nextjournal/clojure-mode": "^0.3.3",
    "install": "^0.13.0",
    "pinia": "^3.0.4",
    "uuid": "^13.0.0",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@pinia/testing": "^1.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "sass": "^1.97.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
